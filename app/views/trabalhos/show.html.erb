<div class="row">
  <div class="col-md-6">
    <h1><%= fa_icon 'eye' %> Detalhar Trabalho</h1>
  </div>
  <div class="col-md-6">
    <div class="btn-group pull-right">
      <a href="<%= @trabalho.download %>" class="btn btn-primary"><%= fa_icon 'download' %> Download</a>
      <a href="<%= trabalhos_path %>" class="btn btn-primary"><%= fa_icon 'undo' %> Voltar</a>
    </div>
  </div>
</div>

<%= render partial: 'detalhes' %>

<% if @trabalho.reprovado? %>
  <%= render partial: 'pareceres' %>
<% end %>
