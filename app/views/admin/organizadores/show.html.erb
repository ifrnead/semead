<div class="row">
  <div class="col-md-6">
    <h1><%= fa_icon 'eye' %> Detalhar Organizador</h1>
  </div>
  <div class="col-md-6">
    <div class="btn-group pull-right">
      <a href="<%= admin_organizadores_path %>" class="btn btn-primary"><%= fa_icon 'undo' %> Voltar</a>
    </div>
  </div>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt><dd><%= @organizador.nome %></dd>
  <dt>E-mail:</dt><dd><%= @organizador.email %></dd>
  <dt>Perfil:</dt><dd><%= @organizador.perfil.nome %></dd>
</dl>
